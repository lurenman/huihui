<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_pay_addexist"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.hui2020.huihui.Payment.Pay_addexistActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/appColor">
        <include
            android:id="@+id/back"
            layout="@layout/c_back" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="从名片库导入"
            android:textColor="#FFFFFF"
            android:textSize="@dimen/fontd" />
        <TextView
            android:id="@+id/pay_addexist_confirm"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="确认"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            android:textSize="@dimen/fontc"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:textColor="#FFFFFF"/>
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/pay_addexist_lin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        </LinearLayout>
    </ScrollView>


</LinearLayout>
